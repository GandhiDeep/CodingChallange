        "class Solution:\n",
        "    def isPalindrome(self, x: int) -> bool:\n",
        "        s = str(x)[::-1]\n",
        "        if(str(x)==s):\n",
        "            return True\n",
        "        else:\n",
        "            return False\n",
        "        "
      ]
    }
