      "source": [
        "t = int(input())\n",
        "while t > 0:\n",
        "  n,k = map(int, input().split(\" \"))\n",
        "  arr = list(map(int, input().split(\" \")))\n",
        "  ans = \"\"\n",
        "  for i in range(n):\n",
        "    if(arr[i]%k==0):\n",
        "      ans = ans + '1'\n",
        "    else:\n",
        "      ans = ans + '0'\n",
        "  print(ans)\n",
        "  t = t-1"
      ]
    }
