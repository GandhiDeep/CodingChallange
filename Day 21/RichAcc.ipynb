      "source": [
        "class Solution:\n",
        "    def maximumWealth(self, accounts: List[List[int]]) -> int:\n",
        "        ans = []\n",
        "        for i in range(len(accounts)):\n",
        "            ans.append(sum(accounts[i]))\n",
        "        return max(ans)"
      ]
    }
  ]
