        "# cook your dish here\n",
        "t = int(input())\n",
        "while t > 0:\n",
        "    a,b,c,d,k = map(int, input().split(\" \"))\n",
        "    dis = abs(a-c) + abs(b-d)\n",
        "    x = k-dis\n",
        "    if(k>=dis and x%2==0):\n",
        "        print(\"Yes\")\n",
        "    else:\n",
        "        print(\"No\")\n",
        "    t =t-1"
      ]
