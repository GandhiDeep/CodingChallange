        "# cook your dish here\n",
        "t = int(input())\n",
        "while t > 0:\n",
        "    e,k = map(int, input().split(\" \"))\n",
        "    count = 0\n",
        "    while e > 0:\n",
        "        e = int(e/k)\n",
        "        count = count + 1\n",
        "    print(count)\n",
        "    t = t-1"
