      "source": [
        "# cook your dish here\n",
        "t = int(input())\n",
        "while t > 0:\n",
        "    a,b,x = map(int, input().split(\" \"))\n",
        "    diff = b-a\n",
        "    if(diff%x==0):\n",
        "        print(int(diff/x))\n",
        "    else:\n",
        "        print(int(diff/x)+1)\n",
        "    t = t-1"
      ]
    }
