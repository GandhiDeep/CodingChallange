        "t = int(input())\n",
        "while t > 0:\n",
        "    n,a,b,c = map(int, input().split(\" \"))\n",
        "    if(b<n or a+c <n):\n",
        "        print(\"NO\")\n",
        "    else:\n",
        "        print(\"YES\")\n",
        "    t =t-1"
