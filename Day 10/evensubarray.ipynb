        "# cook your dish here\n",
        "t = int(input())\n",
        "while t > 0:\n",
        "    n = int(input())\n",
        "    ans = (n*(n+1))/2\n",
        "    if(ans%2==0):\n",
        "        print(n)\n",
        "    else:\n",
        "        print(n-1)\n",
        "    t = t-1"
