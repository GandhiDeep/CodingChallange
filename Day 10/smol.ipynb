      "source": [
        "# cook your dish here\n",
        "t = int(input())\n",
        "while t > 0:\n",
        "    n,k = map(int,input().split(\" \"))\n",
        "    if(n<k or k==0):\n",
        "        print(n)\n",
        "    else:\n",
        "        x = int(n/k)\n",
        "        ans = n-x*k\n",
        "        print(ans)\n",
        "    t = t-1"
      ]
    }
  ]
