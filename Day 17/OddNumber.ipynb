      "source": [
        "class Solution:\n",
        "    def countOdds(self, low: int, high: int) -> int:\n",
        "        if(low%2==0 and high%2==0):\n",
        "            return int(((high-low)/2))\n",
        "        else:\n",
        "            return int(((high-low)/2)+1)"
      ]
    }
  ]
